{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": [
    "import",
    "prettier",
    "react"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "eol-last": 0,
    "no-console": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "jsxBracketSameLine": true,
        "semi": true,
        "bracketSpacing": true
      }
    ]
  }
}